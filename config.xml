<?xml version='1.0' encoding='utf-8'?>
<widget id="com.hyd.metro" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hyderabad Metro</name>
    <description>
        Map of Hyderabad Metro
    </description>
    <author email="k0n3ru@gmail.com" href="http://cordova.io">
        Koneru Dattu
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="http://*.openstreetmap.*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
	
	<!-- Android preferences -->
	<preference name="KeepRunning" value="false"/>
	<preference name="LoadingDialog" value="Drawing the map..."/>
	<preference name="ErrorUrl" value="ErrorPage.html"/>
	
	<icon src="res/icon.png" />
    </platform>
    
    <!-- Global preferences -->
    <preference name="Fullscreen" value="true" />
    <preference name="HideKeyboardFormAccessoryBar" value="true"/>
    
    
</widget>
